@{
  Layout = "_Layout";
  ViewBag.Title = "Factory Home";
  var engineers = ViewBag.Engineers as List<Engineer>;
  var machines = ViewBag.Machines as List<Machine>;

}
@using Factory

<h1>Welcome to the Dr. Sillystringz's Factory</h1>
<h2>Engineers</h2>
<ul>
  @foreach (var engineer in engineers)
  {
    <li>@engineer.PersonName </li>
  }
</ul>
<h2>Machines</h2>
<ul>
  @foreach (var machine in machines)
  {
    <li>@machine.MachineName</li>
  }
</ul>
<p>@Html.ActionLink("See all engineers", "Index", "Engineers")</p>
<p>@Html.ActionLink("See all machines", "Index", "Machines")</p>
